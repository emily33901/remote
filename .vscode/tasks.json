{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				// "isDefault": true
			},
			"label": "rust: cargo build"
		},
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--release"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build release"
		}
	]
}